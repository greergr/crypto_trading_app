{
  "version": 2,
  "buildCommand": "git clone https://github.com/flutter/flutter.git && export PATH=\"$PWD/flutter/bin:$PATH\" && flutter config --no-analytics && flutter pub get && flutter build web --release --web-renderer html",
  "outputDirectory": "build/web",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
