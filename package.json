{
  "name": "crypto_trading_app",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "setup-flutter": "git clone https://github.com/flutter/flutter.git",
    "set-path": "export PATH=\"$PWD/flutter/bin:$PATH\" && flutter doctor",
    "build-web": "flutter build web --release --web-renderer html",
    "vercel-build": "npm run setup-flutter && npm run set-path && flutter pub get && npm run build-web"
  }
}
